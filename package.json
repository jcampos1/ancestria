{
  "name": "rosy_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "daemonite-material": "^4.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "final-form": "^4.10.0",
    "i18next": "^12.0.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-redux-languagedetector": "^1.1.0",
    "local-storage": "^1.4.2",
    "nock": "^10.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-final-form": "^3.6.7",
    "react-i18next": "^8.3.8",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "sinon": "^7.1.1",
    "yup": "^0.26.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/* --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --coverage --coverageDirectory=coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.10.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3"
  }
}
